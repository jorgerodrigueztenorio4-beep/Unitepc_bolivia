                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select th.id_horario,th.gestion ,th.ci_doc  ,(tp.nombres+' '+tp.primerApellido +' '+tp.segundoApellido )as docente,a.nom_aula_lab , th.hora_inicio ,th.hora_fin,th.dia,th.tipo_clase  ,p.SiglaP,p.Materia,p.Sis_plan
from tb_horario th
 join tb_designacion td on td.id_designacion  = th.id_horario 
 join tb_personal tp on th.ci_doc = tp.ci 
 join planes_estudios p on td.id_materia = p.id_plan
 join tb_aula a on th.aula = a.id_nomal  
  join tb_sedes s on th.sede = s.idSede 
 join tb_carreras c on p.codcar = c.CARSIGLA </CommandText>
