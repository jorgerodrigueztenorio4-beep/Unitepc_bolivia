.calendar-modern {
    border: 1px solid #dee2e6;
    border-radius: 10px;
    padding: 8px;
    background: white;
    box-shadow: 0 4px 10px rgba(0,0,0,.08);
    font-family: 'Segoe UI', sans-serif;
}

/* Encabezado mes/año */
.calendar-modern th {
    background: #0d6efd;
    color: white;
    padding: 6px;
    border-radius: 6px;
}

/* Días */
.calendar-modern td {
    padding: 6px;
    text-align: center;
    border-radius: 6px;
    cursor: pointer;
}

/* Hover día */
.calendar-modern td:hover {
    background: #e9f2ff;
}

/* Día seleccionado */
.calendar-modern .SelectedDayStyle {
    background: #0d6efd !important;
    color: white !important;
    font-weight: bold;
}

/* Día de hoy */
.calendar-modern .TodayDayStyle {
    background: #ffc107;
    color: black;
    font-weight: bold;
}

/* Otros meses */
.calendar-modern .OtherMonthDayStyle {
    color: #adb5bd;
}



